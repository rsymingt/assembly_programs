	ORG    $8000
START   MOVEA.L #STRING1,A0
	MOVEA.L #STRING2,A1
	MOVEA.L #MATCH,A2

LOOP	MOVE.B (A0)+,D0
	
	CMP.B (A1)+,D0
	BEQ SKIP
	MOVE.B #$00,MATCH
SKIP	CMPI.B #$0D,D0
	BEQ	EXIT
	CMPI.B #$FF,MATCH
	BEQ	LOOP

EXIT    TRAP #14

        ORG  $9000
STRING1	DC.B 'HELLO'
	DC.B $0D

	ORG  $9020
STRING2 DC.B 'YELLO'
	DC.B $0D

	ORG  $9040
MATCH	DC.B $FF

	END    START